{
  "name": "ts-events-emitter",
  "version": "1.0.1",
  "description": "A typesafe event emitter for typescript.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "build": "rm -rf dist && prettier --write src/ && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shellord/ts-event-emitter.git"
  },
  "keywords": [
    "event emitter",
    "emitter",
    "event"
  ],
  "author": "Saheen Shoukath",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shellord/ts-event-emitter/issues"
  },
  "homepage": "https://github.com/shellord/ts-event-emitter#readme",
  "devDependencies": {
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
